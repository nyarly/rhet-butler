<script type="text/javascript">
  var sock = new SockJS("/live/follower");

  sock.onopen = function() {
    console.log("open");
  };

  sock.onmessage = function(e) {
    console.log("message", e.data);
    impress().goto(e.data);
  };

  sock.onclose = function() {
    console.log("close");
  };
</script>
